<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata>
    <id>Kiwi.Markdown</id>    
    <title>Kiwi.Markdown</title>
	<version>0.0.0.0</version>
	<authors>danielwertheim</authors>
	<copyright>Daniel Wertheim</copyright>
	<summary>Kiwi.Markdown helps you embed your GitHub wiki locally by transforming GitHub flavored Markdown to HTML with support for syntax highlighting.</summary>
    <description>
		Kiwi.Markdown helps you embed your GitHub wiki locally by transforming GitHub flavored Markdown to HTML with support for syntax highlighting.
		Useful in e.g an ASP.Net MVC app. I'm using my GitHub Wiki and also want the wiki documentation at the project website as well. Kiwi helps me with that.
	</description>
	<projectUrl>http://github.com/danielwertheim/kiwi</projectUrl>
	<licenseUrl>http://github.com/danielwertheim/kiwi/blob/master/Source/License-Kiwi.txt</licenseUrl>
	<requireLicenseAcceptance>false</requireLicenseAcceptance>
	<tags>wiki,github,markdown,syntax-highlighting</tags>
	<dependencies>
	  <dependency id="ColorCode" version="1.0.1" />
      <dependency id="MarkdownSharp" version="1.13.0.0" />
	</dependencies>
  </metadata>
  <files>
    <file src="**\Kiwi*.dll" target="lib\net40\"/>
	<file src="**\Kiwi*.xml" target="lib\net40\"/>
  </files>  
</package>