<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata>
    <id>Kiwi</id>    
    <title>Kiwi</title>
	<version>0.0.0.0</version>
	<authors>Daniel Wertheim - @@danielwertheim - http://daniel.wertheim.se</authors>
	<owners>Daniel Wertheim - @@danielwertheim - http://daniel.wertheim.se</owners>
	<summary>Kiwi helps you embed your GitHub wiki locally, e.g in an ASP.Net MVC app.</summary>
