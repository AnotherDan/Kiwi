<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata>
    <id>Kiwi.Mvc3</id>    
    <title>Kiwi.Mvc3</title>
	<version>0.0.0.0</version>
	<authors>danielwertheim</authors>
	<copyright>Daniel Wertheim</copyright>
	<summary>Kiwi.Markdown helps you embed your GitHub wiki locally by transforming GitHub flavored Markdown to HTML with support for syntax highlighting.</summary>
    <description>
		This Content package sets up the necessary parts for using Kiwi.Markdown in an ASP.Net MVC 3 app.
	</description>
	<projectUrl>http://github.com/danielwertheim/kiwi</projectUrl>
	<licenseUrl>http://github.com/danielwertheim/kiwi/blob/master/Source/License-Kiwi.txt</licenseUrl>
	<requireLicenseAcceptance>false</requireLicenseAcceptance>
	<tags>wiki,github,markdown,syntax-highlighting,aspnet,mvc3</tags>
	<dependencies>
	  <dependency id="Kiwi.Markdown" version="$version$" />
      <dependency id="WebActivator" version="1.5" />
	</dependencies>
  </metadata>
  <files>
      <file src="Projects\Kiwi.Mvc3\App_Data\**\*.md" target="Content\App_Data" />
	  <file src="Projects\Kiwi.Mvc3\App_Start\**\*.pp" target="Content\App_Start" />
	  <file src="Projects\Kiwi.Mvc3\Controllers\**\*.pp" target="Content\Controllers" />
	  <file src="Projects\Kiwi.Mvc3\Views\**\*.pp" target="Content\Views" />
	  <file src="Projects\Kiwi.Mvc3\*.transform" target="Content\" />
	  <file src="Projects\Kiwi.Mvc3\*.pp" target="Content\" />
  </files>
</package>